);         }          function luckySearch() {             const queries = ['–ò–Ω—Ç–µ—Ä–µ—Å–Ω—ã–µ —Ñ–∞–∫—Ç—ã', '–°–ª—É—á–∞–π–Ω–∞—è —Å—Ç–∞—Ç—å—è', '–£–¥–∏–≤–∏—Ç–µ–ª—å–Ω—ã–µ –º–µ—Å—Ç–∞'];             searchInput.value = queries[Math.floor(Math.random() * queries.length)];             search();         }          function aiSearch() {             const query = searchInput.value.trim() || '–†–∞—Å—Å–∫–∞–∂–∏ —á—Ç–æ-–Ω–∏–±—É–¥—å –∏–Ω—Ç–µ—Ä–µ—Å–Ω–æ–µ';             alert('AI –ê—Å—Å–∏—Å—Ç–µ–Ω—Ç –¥—É–º–∞–µ—Ç –Ω–∞–¥: "' + query + '"');         }          function selectSuggestion(text) {             searchInput.value = text;             suggestions.classList.remove('active');             search();         }          function toggleVoice(btn) {             btn.classList.toggle('listening');             if (btn.classList.contains('listening')) {                 searchInput.placeholder = '–°–ª—É—à–∞—é –≤–∞—Å...';                 setTimeout(() => {                     btn.classList.remove('listening');                     searchInput.placeholder = '–°–ø—Ä–æ—Å–∏—Ç–µ —á—Ç–æ —É–≥–æ–¥–Ω–æ...';                     searchInput.value = '–ü—Ä–∏–≤–µ—Ç, –∫–∞–∫ –¥–µ–ª–∞?';                 }, 3000);             }         }          function toggleTheme() {             isLightMode = !isLightMode;             document.body.classList.toggle('light-mode');             document.getElementById('theme-icon').className = isLightMode ? 'fas fa-sun' : 'fas fa-moon';         }          searchInput.addEventListener('keypress', (e) => { if (e.key === 'Enter') search(); });          const placeholders = ['–°–ø—Ä–æ—Å–∏—Ç–µ —á—Ç–æ —É–≥–æ–¥–Ω–æ...', '–ù–∞–π—Ç–∏ —Ä–µ—Ü–µ–ø—Ç –ø–∏—Ü—Ü—ã...', '–ö–∞–∫ –≤—ã—É—á–∏—Ç—å Python?', '–õ—É—á—à–∏–µ —Ñ–∏–ª—å–º—ã 2026...', '–ü–æ–≥–æ–¥–∞ –Ω–∞ –∑–∞–≤—Ç—Ä–∞...'];         let placeholderIndex = 0;         setInterval(() => {             placeholderIndex = (placeholderIndex + 1) % placeholders.length;             searchInput.placeholder = placeholders[placeholderIndex];         }